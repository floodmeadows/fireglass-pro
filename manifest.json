{
    "manifest_version": 2,
    "name":             "Fireglass Pro for Fieldglass Timesheets",
    "short_name":       "Fireglass Pro",
    "description":      "Adds some missing features to make it quicker and easier to fill in your timesheets.",
    "version":          "1.0",
    "browser_action":   {
    },
    "content_scripts":  [{
        "exclude_globs": [ ],
        "include_globs": ["*"],
        "js":            ["fireglass-pro.user.js"],
        "matches":       ["https://www.fieldglass.net/rate_schedule_time_sheet_form.do*"],
        "run_at": "document_end"
    }],
    "icons": {
      "16": "images/fireglass-pro-icon16.png",
      "32": "images/fireglass-pro-icon32.png",
      "48": "images/fireglass-pro-icon48.png",
      "128": "images/fireglass-pro-icon128.png"
    },
    "converted_from_user_script": true
}
